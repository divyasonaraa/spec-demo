{
  "name": "dynamic-form-genrator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "sync-validate": "node tools/sync-and-validate-simple.mjs",
    "validate": "node tools/debugger/cli/run-debugger.mjs",
    "validate:samples": "for f in src/config/samples/*.ts; do node tools/debugger/cli/run-debugger-config.mjs \"$f\" || true; done",
    "precommit": "npm run sync-validate"
  },
  "dependencies": {
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vue-tsc": "^3.1.4"
  }
}
